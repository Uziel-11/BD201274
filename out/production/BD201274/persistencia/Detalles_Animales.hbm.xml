<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="persistencia">
    <class name="Detalles_Animales" table="clientes" >
        <id  name="idDetalles_Animales"
             column="idClientes"
             type="java.lang.Integer"
             unsaved-value="null">
            <generator class="native">
            </generator>
        </id>

        <property name="codigo_tratamiento"  column="Nombre" />
        <property name="codigo_Medicacion"  column="Celular"/>
        <property name="precio_Tratamiento"  column="Direccion"/>
        <property name="precio_Medicacion"  column="Ciudad" />
    </class>
</hibernate-mapping>