<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="persistencia">
    <class name="Citas" table="citas" >
        <id  name="idCitas"
             column="IdCitas"
             type="java.lang.Integer"
             unsaved-value="null">
            <generator class="native">
            </generator>
        </id>

        <property name="fecha_Citas" column="Fecha_Citas"/>
        <property name="total_Factura"  column="Total_Factura" />
        <property name="forma_Pago"  column="Forma_Pago"/>
        <property name="cantidad_Pagada"  column="Cantidad_Pagada"/>
        <property name="facturacion"  column="Facturacion" />
    </class>
</hibernate-mapping>